var estacion = "verano";
switch (estacion) {
  case "verano":
    System.put.print ("es verano");
    break;
  case "invierno":
    System.put.print ("es invierno");
    break;
  case "otoño":
    System.put.print ("es otoño");
    break;
  case "primavera":
    System.put.print ("es primavera");
    break;
  default:
    System.put.print ("el valor de una variable no es de una estacion");
  
