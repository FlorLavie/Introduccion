int numeroif = 3;
if (numeroif>0) {
    System.out.println ("Es positivo");
  } else if (numeroif < 0) {
    System.out.println ("es negativo");
  } else {
    System.out.println ("es 0");
}

int numeroWhile = 1;
while (numeroWhile < 3) {
  numeroWhile++:
  System.out.println (numeroWhile);
}

int numeroWhile = 1;
while (numeroWhile < 3) {
  numeroWhile++:
  System.out.println (numeroWhile);
}

